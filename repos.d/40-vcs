# -*- mode: sh -*-

#
# 40. VCS
#

[$HOME/repos/vcs/git]
checkout = git clone git@rpi3.lan:repos/git.git
fixups =
  git_ensure_remote upstream git@github:git/git.git
  make configure
  ./configure --prefix=/usr.home.local
  make all doc
  make install 
# make install-doc install-html
skip = true

