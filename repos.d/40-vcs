# -*- mode: sh -*-

#
# 40. VCS
#

[$HOME/repos/vcs/git]
checkout = git clone git@rpi3.lan:git.git
fixups =
  git_ensure_remote upstream git@github.com:git/git.git
  [ -f ./configure ] || autoreconf -fi
  make configure
  ./configure --prefix=/home/wandsas/git
  make all doc
  make install 
# make install-doc install-html

